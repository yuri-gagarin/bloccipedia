<h1>Add User</h1>

<%= form_for [@wiki, @collaborator] do |f| %>

<div class="form-group">
  <%= f.collection_select :user_id, available_users(@wiki), :id, :email, :include_blank => 'Select User' %>
</div>

<div class="form-group">
  <%= f.submit "Save", class: 'green-btn centered-text btn-success' %>
</div>

<% end %>
